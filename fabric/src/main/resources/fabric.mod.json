{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": ["${modAuthor}"],
  "license": "${modLicense}",
  "icon": "assets/bluemap-journeymap-connector/icon.jpg",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.machinepeople.bluemapjourneymapconnector.BlueMapJourneyMapConnectorMod"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.machinepeople.bluemapjourneymapconnector.client.BlueMapJourneyMapConnectorClient"
      }
    ],
    "journeymap": [
      "com.machinepeople.bluemapjourneymapconnector.journeymap.JourneyMapIntegration"
    ]
  },
  "mixins": [],
  "depends": {
    "fabricloader": ">=0.16.0",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": ">=${minecraftVersion}"
  },
  "suggests": {
    "journeymap": "*",
    "bluemap": "*"
  },
  "custom": {
    "journeymap": {
      "plugins": [
        "com.machinepeople.bluemapjourneymapconnector.journeymap.JourneyMapIntegration"
      ]
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/your-discord"
      }
    }
  }
}

